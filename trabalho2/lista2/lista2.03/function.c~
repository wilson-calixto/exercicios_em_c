int funcPrimo(int a)
{
    int  i,  vezes = 0, valor;
    for (i = 1; i <= a && vezes <= 3; i++)               
    //for (i = 1; i <= num; i++)
    {  
        if (a % i == 0)
        vezes ++;
    }
    
    if (vezes == 2)
    {
     valor = 1;
    }
    else  
    {
     valor = 0;
    }
    
    return valor;
}

