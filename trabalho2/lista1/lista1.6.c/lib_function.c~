int is_hour(int hora){
    if (hora <= 0 && hora < 23 ){
        return 1;
    }else{
        return 0;
    }
}

int is_minute(int minuto){
    if(minuto <= 0 && minuto < 60){
        return 1;
    }else{
        return 0;
    }
}

