#include <stdio.h>
//faz converÃ£o de temperatura de celsius para farenheit e farenheit/celsius.
void celsius (float x)
{
    float c;
    c = ( 5*(x-32)/9 );
    printf ( "%f\n",c);//resultado em celsius 
}

void farenheit (float x)
{
    float f;
    f = ( 9*x/5 ) + 32;
    printf ( "%f\n",f);//resultado em Farenheit
}
